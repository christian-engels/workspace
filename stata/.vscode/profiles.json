{
    "profiles": [
        {
            "name": "Stata",
            "extensions": [
                "kylebarron.stata-enhanced",
                "StataEditor.stata-language",
                "StataEditor.stataRun",
                "GitHub.copilot",
                "GitHub.copilot-chat"
            ],
            "settings": {
                "editor.wordWrap": "on",
                "files.associations": {
                    "*.do": "stata",
                    "*.ado": "stata"
                },
                "editor.formatOnSave": true,
                "editor.tabSize": 4
            },
            "keybindings": {
                "stata.runSelection": "ctrl+enter",
                "stata.runFile": "ctrl+shift+enter"
            }
        }
    ]
}